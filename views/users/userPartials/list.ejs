<div class="list-group mb-2" id="list-tab" role="tablist">
  <!-- Favorited School List -->
  <a
    class="list-group-item list-group-item-action active"
    id="list-favorited-list"
    data-toggle="list"
    href="#list-favorited"
    role="tab"
    aria-controls="favorited"
  >
    <i class="fas fa-thumbs-up"></i> Favorite School
  </a>
  <!-- Favorited School List End -->

  <!-- Review List -->
  <a
    class="list-group-item list-group-item-action"
    id="list-reviewed-list"
    data-toggle="list"
    href="#list-reviewed"
    role="tab"
    aria-controls="reviewed"
  >
    <i class="fas fa-comment-dots"></i> Reviewed
  </a>
  <!-- Review List End -->
</div>